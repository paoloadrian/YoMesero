<p id="notice"><%= notice %></p>

<h1>Listing Item Rankings</h1>

<table>
  <thead>
    <tr>
      <th>Rate</th>
      <th>Item</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @item_rankings.each do |item_ranking| %>
      <tr>
        <td><%= item_ranking.rate %></td>
        <td><%= item_ranking.item %></td>
        <td><%= link_to 'Show', item_ranking %></td>
        <td><%= link_to 'Edit', edit_item_ranking_path(item_ranking) %></td>
        <td><%= link_to 'Destroy', item_ranking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item ranking', new_item_ranking_path %>
